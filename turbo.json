{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      },
      "vercel-dev": {
        "cache": false,
        "outputs": [".vercel/**", "!.vercel/cache/**"]
      }
    }
  }